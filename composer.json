{
	"name": "thenandan/therepository",
	"description": "Awesome Laravel Repository",
	"type": "laravel-library",
	"license": "proprietary",
	"authors": [
		{
			"name": "Keshari Nandan",
			"email": "thenandan4@gmail.com",
			"homepage": "https://github.com/thenandan"
		}
	],
	"minimum-stability": "stable",
	"prefer-stable": true,
	"config": {
		"secure-http": false,
		"preferred-install": {
			"*": "dist"
		},
		"github-protocols": [
			"git",
			"https"
		],
		"discard-changes": true,
		"optimize-autoloader": true,
		"classmap-authoritative": true
	},
	"require": {
		"php": "^7.1.3"
	},
	"require-dev": {
		"laravel/framework": "^5.6|^5.7|^5.8|^6.0|^7.0",
		"illuminate/support": "^5.5|^6"
	},
	"autoload": {
		"psr-4": {
			"TheNandan\\TheRepository\\": "src/"
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"TheNandan\\TheRepository\\TheRepositoryServiceProvider"
			]
		}
	}
}
