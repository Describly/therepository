{
	"name": "thenandan/therepository",
	"description": "Awesome Laravel Repository",
	"type": "library",
	"license": "proprietary",
	"authors": [
		{
			"name": "Keshari Nandan",
			"email": "thenandan4@gmail.com",
			"homepage": "https://github.com/thenandan"
		}
	],
	"minimum-stability": "stable",
	"prefer-stable": true,
	"config": {
		"platform": {
			"php": "^7.1"
		},
		"secure-http": false,
		"preferred-install": {
			"*": "dist"
		},
		"github-protocols": [
			"git",
			"https"
		],
		"discard-changes": true,
		"optimize-autoloader": true,
		"classmap-authoritative": true
	},
	"require": {
		"php": ">=7.1",
		"illuminate/support": "^5.5|^6",
		"laravel/framework": "5.7.*"
	},
	"require-dev": {
	}
}
